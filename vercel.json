{
  "version": 2,
  "headers": [
    {
      "source": "/assets/music/.*\\.mp3$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "audio/mpeg"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        }
      ]
    },
    {
      "source": "/assets/music/.*\\.wav$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "audio/wav"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        }
      ]
    },
    {
      "source": "/assets/music/.*\\.m4a$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "audio/mp4"
        }
      ]
    },
    {
      "source": "/assets/music/.*\\.aac$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "audio/aac"
        }
      ]
    }
  ]
}
